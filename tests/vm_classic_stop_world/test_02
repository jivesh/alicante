P = parse_and_compile(`
const pair = (a, b) => f => f(a, b);
const head = p => p((h, t) => h);
const tail = p => p((h, t) => t);

let count = 4;

function placeholder(n) {
    const garbage = pair(pair(1, 2), pair(3, 4));
    return n;
}

while (count > 0) {
    count = placeholder(count) - 1;
}

5;
`);

initialize_machine(500);
run();